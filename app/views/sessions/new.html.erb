<h1>Welcome to URLockbox</h1>

<div id="login">
  <h3>Sign In</h3>
  <%= form_for :session, url: login_path do |f| %>
    <%=  f.label :email %>
    <%=  f.text_field :email%>
    <%=  f.label :password %>
    <%=  f.password_field :password %>
    <%= f.submit "Log In" %>
  <% end %>

  <p>New user?</p>
  <a href="/users/new">Sign up!</a>
</div>
